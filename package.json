{
  "name": "react-passport",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:production": "npm run lint && npm run build:client:production && npm run build:server:production",
    "build:client": "NODE_ENV=development webpack --config scripts/webpack/config.js -d",
    "build:server": "NODE_ENV=development ./node_modules/.bin/babel -d build/server src/server",
    "build:client:production": "NODE_ENV=production webpack --config scripts/webpack/config.js -d",
    "build:server:production": "NODE_ENV=production ./node_modules/.bin/babel -d build/server src/server",
    "dev:server": "NODE_ENV=development nodemon --watch src/server --watch config src/server/index.js --exec babel-node",
    "dev:client": "NODE_ENV=development webpack-dev-server --config scripts/webpack/config.js --hot --disable-host-check --content-base build",
    "lint": "eslint src",
    "server": "NODE_ENV=development node build/server",
    "server:production": "NODE_ENV=production node build/server",
    "start": "npm run server:production",
    "dev": "npm run dev:server & npm run dev:client",
    "production": "npm run build:production && npm start"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-beta.3",
    "compression": "^1.7.1",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "method-override": "^2.3.10",
    "mongoose": "^4.13.8",
    "passport": "^0.4.0",
    "passport-dropbox-oauth2": "^1.1.0",
    "passport-facebook": "^2.1.1",
    "passport-flickr": "^0.2.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-linkedin": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-pinterest": "^1.0.0",
    "passport-reddit": "^0.2.4",
    "passport-stack-exchange": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "passport-vkontakte": "^0.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.6",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nodemon": "^1.14.11",
    "process-env": "^1.1.0",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
